{
  "compilerOptions": {
    "inlineSources": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "target": "es5",
    "moduleResolution": "node",
    "module": "commonjs",
    "sourceMap": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": [
      "esnext", "dom"
    ]
  }
}
