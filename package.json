{
  "name": "graphql-mock",
  "version": "0.0.1",
  "description": "A GraphQL server that mocks response based on a JSON schema",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "scripts": {
    "start": "nodemon -e ts --exec 'ts-node src/index.ts'",
    "debug": "ts-node --inspect --compilerOptions '{\"inlineSources\":true}' src/index.ts",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lemuelbarango/graphql-mock.git"
  },
  "author": "Lemuel Barango",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lemuelbarango/graphql-mock/issues"
  },
  "homepage": "https://github.com/lemuelbarango/graphql-mock#readme",
  "dependencies": {
    "express": "^4.16.4",
    "express-graphql": "^0.8.0",
    "faker": "^4.1.0",
    "graphql": "^14.3.0",
    "graphql-tools": "^4.0.4",
    "lodash.get": "^4.4.2"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/express-graphql": "^0.8.0",
    "@types/faker": "^4.1.5",
    "@types/graphql": "^14.2.0",
    "@types/lodash.get": "^4.4.6",
    "nodemon": "^1.19.0",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5"
  }
}
